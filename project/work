
def check_anagram(word1, word2):
    if sorted(word1) == sorted(word2):
        return 'anagrama'
    else:
        return 'anagrama emes'

print(check_anagram('kaban', 'bankai'))


student = {'first_name': 'Azamat', 'last_name': 'Kubatbekov', 'age': 33}


def check_key(word):
    if word in student:
        return student[word]
    else:
        return 'No'

word = input('key: ').lower()

print(check_key(word))


print(merge([1, 7, 10, 16], [5, 6, 13, 20]))

